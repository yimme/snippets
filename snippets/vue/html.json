{
	"template with slot name": {
		"prefix": [
			"template:slot",
		],
		"body": "<template #${1:name}>${2:content}</template>",
		"description": "named slot"
	},
	"template": {
		"prefix": [
			"template:empty"
		],
		"body": "<template>${1:content}</template>",
		"description": "An empty template tag"
	},
	"v-for": {
		"prefix": [
			"vfor",
		],
		"body": [
			"<${1:div} v-for=\"${2:item} in ${2:item}s\" :key=\"${2:item}.id\">",
			"\t{{ ${2:item} }}",
			"</${1:div}>"
		],
		"description": "v-for statement"
	},
	"v-if": {
		"prefix": [
			"vif",
		],
		"body": [
			"<${1:div} v-if=\"${2}\">",
			"\t${3}",
			"</${1:div}>",
		]
	},
	"v-else": {
		"prefix": [
			"velse",
		],
		"body": [
			"<${1:div} v-else=\"${2}\">",
			"\t${3}",
			"</${1:div}>",
		]
	},
	"v-if-else": {
		"prefix": [
			"vifel",
		],
		"body": [
			"<${1:div} v-if=\"${2}\">",
			"\t${3}",
			"</${1:div}>",
			"<${1:div} v-else>",
			"\t${4}",
			"</${1:div}>",
		]
	},
	"v-else-if": {
		"prefix": [
			"velif",
		],
		"body": [
			"<${1:div} v-if=\"${2}\">",
			"\t${3}",
			"</${1:div}>",
			"<${1:div} v-else-if=\"${5}\">",
			"\t${4}",
			"</${1:div}>",
		]
	}
}
